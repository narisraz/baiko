asa esorina(a: Isa, b: Isa): Isa dia
  mamoaka a - b;
farany

asa ampidiana(a: Isa, b: Isa): Isa dia
  mamoaka a * b;
farany

asa zarana(a: Isa, b: Isa): Isa dia
