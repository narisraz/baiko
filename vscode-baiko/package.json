{
  "name": "vscode-baiko",
  "displayName": "Baiko",
  "description": "Fitohana ho an'ny fiteny Baiko â€” fanamarihana, snippets, ary famolavolana",
  "version": "0.1.0",
  "publisher": "baiko",
  "engines": { "vscode": "^1.80.0" },
  "categories": ["Programming Languages"],
  "main": "./extension.js",
  "activationEvents": ["onLanguage:baiko"],
  "scripts": {
    "build": "node build.mjs",
    "vscode:prepublish": "node build.mjs",
    "package": "node build.mjs && npx vsce package --no-dependencies --allow-missing-repository --allow-star-activation"
  },
  "devDependencies": {
    "@vscode/vsce": "^3.0.0",
    "esbuild": "^0.25.0",
    "typescript": "^5.0.0"
  },
  "contributes": {
    "languages": [
      {
        "id": "baiko",
        "aliases": ["Baiko", "baiko"],
        "extensions": [".baiko"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "baiko",
        "scopeName": "source.baiko",
        "path": "./syntaxes/baiko.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "baiko",
        "path": "./snippets/baiko.json"
      }
    ]
  }
}
